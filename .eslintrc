{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "airbnb/hooks"],
  "globals": {
    "config": false
  },
  "parser": "babel-eslint",
  "rules": {
    "prefer-destructuring": "off",
    "react/jsx-one-expression-per-line": "off",
    "jsx-quotes": ["error", "prefer-single"],
    "quotes": ["error", "single"]
  }
}